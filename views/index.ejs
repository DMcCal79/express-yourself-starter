<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <title>My First Express App</title>
  </head>
  <body>
    <img src= <%=avatar %> />
    <h1><%= description %></h1>
    <h3>My skills are:</h3>
    <ul>
      <% skills.forEach(function(skill) {%>
        <li><%=skill%></li>
        <% }) %>
    </ul>
    <h3>My hobbies are:</h3>
    <ul>
      <% hobbies.forEach(function(hobby) {%>
        <li><%=hobby%></li>
        <% }) %>
    </ul>
  </body>
  </html>
